# Prime Factor Decomposition
def pfd(n):
    prime_factors = []
    i = 2
    # ‚±‚±‚ğ‘f”‚¾‚¯‚Å‰ñ‚¹‚Î‚à‚Á‚Æ‘¬‚­‚È‚é
    while True:
        if n == 1: break
        # Š„‚è‘±‚¯‚Ä‘fˆö”i‚ğæ‚èœ‚­
        if n%i == 0:
            count = 0
            while n%i ==0:
                n = n/i
                count += 1
            prime_factors.append([i, count])
        i += 1
    return prime_factors

print pfd(600851475143)
